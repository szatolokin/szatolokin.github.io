include mixin/pug/variables
include mixin/pug/mixins

doctype html
html(lang="ru")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Document
    link(rel="stylesheet", href="css/main.css")
  body
    .wrapper
      - for (let i = 0; i < 20; i++)
        ul.pets
          each pet in pets
            +pet(pet)

    block scripts
      script(src="js/modules/jquery.min.js")
      script(src="js/main.dev.js")